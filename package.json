{
  "name": "wonder-editor",
  "version": "0.13.1",
  "authors": "Wonder",
  "license": "GPL-3.0",
  "repository": "git@github.com:Wonder-Technology/Wonder-Editor.git",
  "homepage": "https://github.com/Wonder-Technology/Wonder-Editor.js",
  "scripts": {
    "start": "node ./server/app.js",
    "bsb:watch": "bsb -clean-world -make-world -w",
    "bsb:build": "bsb -clean-world -make-world",
    "webpack": "webpack",
    "watch": "gulp watch",
    "build": "gulp build",
    "test:watch": "gulp changeSnapshotPath && jest --maxWorkers=4 --watch --config jest.json",
    "test:update": "gulp changeSnapshotPath && jest --maxWorkers=4 -u --config jest.json",
    "test:coverage": "gulp changeSnapshotPath && jest --maxWorkers=4 --config jest_coverage.json || true",
    "test:ci": "npm run bsb:build && gulp changeSnapshotPath && jest -u --maxWorkers=4 --config jest_coverage.json || true",
    "codecov": "cat coverage/lcov.info | codecov",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "updateVersion": "gulp updateVersion"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "keywords": [
    "wonder",
    "functional-programming",
    "3d",
    "editor",
    "reasonml",
    "reason",
    "bucklescript"
  ],
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bs-platform": "4.0.0",
    "codecov.io": "^0.1.6",
    "cz-customizable": "^5.2.0",
    "file-save": "^0.2.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "gulp-sync": "^0.1.4",
    "jest": "^23.5.0",
    "jszip": "^3.1.5",
    "react-addons-css-transition-group": "^15.6.0",
    "sinon": "^4.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "wonder-bs-jest": "^0.2.18",
    "wonder-bs-sinon": "^0.1.7"
  },
  "dependencies": {
    "antd": "2.13.11",
    "bs-fetch": "^0.3.0",
    "express": "^4.16.1",
    "file-saver": "^1.3.8",
    "hotkeys-js": "^3.4.3",
    "react": "15.6.2",
    "react-color": "^2.14.1",
    "react-dom": "15.6.2",
    "react-test-renderer": "15.5.4",
    "reason-react": "0.4.2",
    "wonder-bs-json": "^0.0.1",
    "wonder-bs-jszip": "0.0.2",
    "wonder-bs-most": "^0.0.3",
    "wonder-commonlib": "0.2.23",
    "wonder-log": "^0.2.10",
    "wonder.js": "1.0.0-alpha.24.5"
  },
  "resolutions": {
    "wonder-log": "^0.2.10",
    "wonder-webgl": "^0.0.6",
    "wonder-bs-sinon": "0.1.7",
    "component-classes": "1.2.5",
    "wonder-commonlib": "0.2.23",
    "wonder-bs-jest": "0.2.18"
  }
}
